<link rel="import" href="./global.pc" />
<link rel="import" href="./pane.pc" />

<component id="td-windows-pane" [[property windows]]>
  <template>
    <td-pane>
      <span slot="header">Artboards</span>
      <span slot="content">
        <td-list>
          <td-list-item [[repeat windows as window]] key=[[bind window.$id]]>
            [[bind window.document.title || window.location]]
          </td-list-item>
        </td-list>
      </span>
    </td-pane>
  </template>
</component>
