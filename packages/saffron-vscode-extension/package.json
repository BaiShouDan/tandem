{
  "name": "saffron-vscode-extension",
  "displayName": "saffron",
  "description": "Richer experience for Saffron app",
  "version": "0.0.2",
  "publisher": "me",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.previewSaffronDocument"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.previewSaffronDocument",
        "title": "Display Saffron Preview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build-watch": "echo 'cannot build watch'"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "mesh": "^6.0.17",
    "saffron-back-end": "^1.0.1"
  }
}
