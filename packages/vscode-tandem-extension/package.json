{
  "name": "tandem-vscode-extension",
  "displayName": "Tandem",
  "description": "Visual programming for web applications",
  "version": "0.0.21",
  "icon": "assets/logo.png",
  "publisher": "tandemcode",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/tandem.config.js"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openExternalWindow",
        "title": "Open Tandem in external window"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -watch -p ./",
    "build": "tsc",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build-watch": "echo 'cannot build watch'"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/chokidar": "^1.7.2",
    "@types/express": "^4.0.37",
    "@types/get-port": "^0.0.4",
    "@types/http-proxy": "^1.12.1",
    "@types/request": "^2.0.7",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "aerial-common2": "^0.0.1",
    "body-parser": "^1.17.2",
    "connect-multiparty": "^2.0.0",
    "express": "^4.15.4",
    "get-port": "^3.2.0",
    "http-proxy": "^1.16.2",
    "paperclip": "^8.0.7",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "request": "^2.83.0",
    "socket.io-client": "^2.0.4",
    "tandemapp": "^2.0.2",
    "vscode": "^1.1.5"
  },
  "author": "",
  "license": "ISC"
}
