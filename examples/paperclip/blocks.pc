<link rel="stylesheet" type="text/css" href="./style.css" />

<artboard id="repeat">
  ${Array.from(each, () => {
    return children;
  })}
</artboard>

<artboard id="hello" title="Hello World">
  <repeat each=${Array.from({ length: count }, (num) => num)}>
    ${children}
  </repeat>
</artboard>

<artboard title="Hello World" style="top: 211px; left: -4px;">
  <hello message="world" count="30" style="color: red; display: block;">
    <div>Child!</div>
  </hello>
</artboard>