{
    "parser": "espree",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "node": true,
        "browser": true,
        "mocha": true
    },
    "globals": {
        "expect": true
    },
    "extends": [
        "airbnb"
    ],
    "rules": {
        "no-use-before-define": 0,
        "eqeqeq": 0,
        "semi": 2,
        "import/no-unresolved": 0,
        "padded-blocks": 0,
        "no-multi-spaces": 0,
        "no-proto": 0,
        "prefer-rest-params": 0,
        "new-cap": 0,
        "no-trailing-spaces": 0,
        "prefer-template": 0
    }
}