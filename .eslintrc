{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "rules": {
    "object-shorthand": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "max-len": [1, 200],
    "padded-blocks": 0,
    "no-new-func": 0,
    "no-cond-assign": 0,
    "react/no-multi-comp": 0,
    "no-param-reassign": 0,
    "no-multi-spaces": 0,
    "guard-for-in": 0,
    "react/prop-types": 0,
    "eqeqeq": 0,
    "no-proto": 0,
    "prefer-rest-params": 0,
    "no-var": 0,
    "default-case": 0,
    "react/sort-comp": 0,
    "react/jsx-no-bind": 0,
    "jsx-quotes": ["error", "prefer-single"],
    "func-names": 0,
    "no-restricted-syntax": 0,
    "key-spacing": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "react/prefer-stateless-function": 0,
    "no-nested-ternary": 0,
    "prefer-arrow-callback": 0,
    "no-unneeded-ternary": 0,
    "no-confusing-arrow": 0,
    "prefer-template": 0,
    "consistent-return": 0
  },
  "env": {
      "browser": true,
      "node": true,
      "mocha": true
  }
}
